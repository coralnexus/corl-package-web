{
  "builders": {
    "package": {
      "environment": {
        "production": {
          "coralnexus__core": "github:::coralnexus/corl-package-core[6ec8624a9146d579d1284b5495cb100227b9358a]"
        },
        "qa": {
          "use": "production"
        },
        "default": {
          "coralnexus__core": "github:::coralnexus/corl-package-core"
        }
      }
    }
  },
  "provisioners": {
    "puppetnode": {
      "coralnexus::web": {
        "profiles": {
          "apache_server": {
            "environment": {
              "production": {
                "modules": {
                  "apache": "github:::coralnexus/puppet-apache[390725a8ed1a50cb530c6b3199071a1bbc42402e]"
                }
              },
              "qa": {
                "use": "production"
              },
              "default": {
                "modules": {
                  "apache": "github:::coralnexus/puppet-apache"
                }
              }
            }
          },
          "apache_static": {
            "extend": "apache_server"
          },
          "varnish_server": {
            "environment": {
              "production": {
                "modules": {
                  "varnish": "github:::maxchk/puppet-varnish[b9846d1b35e87a45c98213fddfe71fa0a6f3b31c]"
                }
              },
              "qa": {
                "use": "production"
              },
              "default": {
                "modules": {
                  "varnish": "github:::maxchk/puppet-varnish[develop]"
                }
              }
            }
          },
          "apache_icingaweb2": {
            "extend": [
              "apache_server",
              "coralnexus::core::profile::icinga_server",
              "coralnexus::core::profile::php"
            ],
            "environment": {
              "production": {
                "modules": {
                  "icingaweb2": "github:::coralnexus/puppet-icingaweb2"
                }
              },
              "qa": {
                "use": "production"
              },
              "default": {
                "modules": {
                  "icingaweb2": "github:::coralnexus/puppet-icingaweb2"
                }
              }
            }
          }
        }
      }
    }
  }
}
